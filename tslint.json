{
  "extends": "tslint:latest",
  "rules": {
    "interface-name": false,
    "interface-over-type-literal": false,
    "no-implicit-dependencies": [true, "dev"],
    "no-unused-expression": false,
    "variable-name": {
      "options": ["allow-leading-underscore"]
    },
    "no-console": [true, "warning"]
  }
}
